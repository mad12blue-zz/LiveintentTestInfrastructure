<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Liveintent API Test Suite" parallel="classes" thread-count="2" configfailurepolicy="continue">
  <listeners>
    <listener class-name="com.liveintent.configuration.TestRetryAnalyzerListener" />
  </listeners>
  
  <test name="Routing Test">
    <classes>
      <class name="com.liveintent.test.RoutingTest"/>
    </classes>
  </test>

</suite>
